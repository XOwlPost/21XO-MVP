{
    "name": "21XO-MVP",
    "build": {
      "dockerfile": "Dockerfile",
      "context": "..",
      "args": {
        "VARIANT": "11",
        "INSTALL_NODE": "true"
      }
    },
    "settings": {
      "terminal.integrated.shell.linux": "/bin/bash"
    },
    "postCreateCommand": "npm install",
    "remoteUser": "vscode",
    "extensions": [
        "dbaeumer.vscode-eslint",
        "ms-vscode-remote.remote-wsl",
        "ms-python.python",
        "ms-python.vscode-pylance"
      ]      
    "forwardPorts": [3000]
  }
  