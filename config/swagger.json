  "paths": {"paths": {
    "/users": {
      "get": {
        "summary": "Retrieves a list of users",
        "description": "This endpoint retrieves all users.",
        "responses": {
          "200": {
            "description": "A list of users",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/User"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
  },
  "definitions": {"definitions": {
    "User": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "name": {
          "type": "string"
        },
        "email": {
          "type": "string"
        }
      },
      "required": ["id", "name", "email"]
    }
  }
  }
