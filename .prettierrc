{
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "rangeStart": 0,
    "rangeEnd": Infinity,
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf",
    "embeddedLanguageFormatting": "auto",
    "overrides": [
      {
        "files": "*.json",
        "options": {
          "printWidth": 100
        }
      },
      {
        "files": "*.md",
        "options": {
          "proseWrap": "always"
        }
      },
      {
        "files": ["*.yml", "*.yaml"],
        "options": {
          "tabWidth": 2
        }
      }
    ]
  }
  